2',\r\n        icon: Tickets,\r\n        title: '我的订单',\r\n        router: 'OrderCenter',\r\n        Subclass: []\r\n      },\r\n      {\r\n        id: '3',\r\n        icon: LocationInformation,\r\n        title: '邮寄地址',\r\n        router: 'MyAddress',\r\n        Subclass: []\r\n      },\r\n      {\r\n        id: '4',\r\n        icon: PhoneFilled,\r\n        title: '联系客服',\r\n        router: '',\r\n        Subclass: []\r\n      },\r\n      {\r\n        id: '5',\r\n        icon: Help,\r\n        title: '帮助中心',\r\n        router: '',\r\n        Subclass: []\r\n      },\r\n      {\r\n        id: '6',\r\n        icon: InfoFilled,\r\n        title: '关于我们',\r\n        router: '',\r\n        Subclass: []\r\n      },\r\n      {\r\n        id: '7',\r\n        icon: Setting,\r\n        title: '账户设置',\r\n        router: '',\r\n        Subclass: []\r\n      }]\r\n    const menu = reactive(Array)\r\n\r\n\r\n    return { menu, ac_index }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.sidebar-cont {\r\n  position: fixed;\r\n  top: 120px;\r\n  left: 0;\r\n  bottom: 50px;\r\n  background: #ffffff;\r\n  width: 9%;\r\n  height: 100vh;\r\n  color: #000000 !important;\r\n  overflow-y: auto;\r\n  /* 调整样式 */\r\n  background-color: #ffffff;\r\n\r\n}\r\n\r\n/* 调整菜单激活样式 */\r\n.el-menu-item--active {\r\n  background-color: #ffffff;\r\n}\r\n\r\n.router-link-active {\r\n  text-decoration: none;\r\n}\r\n\r\na {\r\n  text-decoration: none;\r\n}\r\n\r\n</style>"],"mappings":"AAqCA,SACEA,OAAO,EACPC,mBAAmB,EACnBC,cAAc,EACdC,WAAW,EACXC,WAAW,EACXC,IAAI,EACJC,UAAU,EACVC,OAAO,EACPC,KAAI,QACC,yBAAwB;AAC/B,SAASC,QAAQ,EAAEC,GAAE,QAAS,KAAI;AAElC,eAAe;EACbC,UAAU,EAAE;IACVX,OAAO,EAAPA,OAAO;IACPC,mBAAmB,EAAnBA,mBAAmB;IACnBC,cAAc,EAAdA,cAAc;IACdC,WAAW,EAAXA,WAAW;IACXC,WAAW,EAAXA,WAAW;IACXC,IAAI,EAAJA,IAAI;IACJC,UAAU,EAAVA,UAAU;IACVC,OAAO,EAAPA,OAAO;IACPC,KAAI,EAAJA;EACF,CAAC;EACDI,KAAK,WAAAA,MAAA,EAAG;IACN,IAAMC,QAAO,GAAIH,GAAG,CAAC,GAAG;IAExB,IAAMI,KAAI,GAAI,CACd;MACIC,EAAE,EAAE,GAAG;MACPC,IAAI,EAAER,KAAK;